{
  "name": "niklang-extantion",
  "displayName": "NikLang Extantion",
  "description": "NikLang Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "NikLang",
        "aliases": [
          "NikLang",
          "NikLang"
        ],
        "extensions": [
          ".nl"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "icon-nl.png",
          "dark": "icon-nl.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "NikLang",
        "scopeName": "source.nl",
        "path": "./syntaxes/NikLang.tmLanguage.json"
      }
    ]
  }
}
